{
    "id": "gravity_well",
    "name": "gravity_well",
    "version": "0.2.2",
    "description": "A tool to import text, markdown, and PDF files as new notes, add NPL derived tags, and track other properties.",
    "main": "main.js",
    "scripts": {
        "eslint": "eslint . --ext .ts,.tsx --fix",
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "gravity",
        "file-importer",
        "txt",
        "pdf",
        "markdown",
        "obsidian",
        "nlp",
        "notes.app"
    ],
    "author": "John Major & Cal",
    "license": "MIT",
    "type": "module",
    "devDependencies": {
        "@types/natural": "^5.1.5",
        "@types/node": "20.11.6",
        "@typescript-eslint/eslint-plugin": "6.19.1",
        "@typescript-eslint/parser": "6.19.1",
        "builtin-modules": "3.3.0",
        "esbuild": "0.21.3",
        "esbuild-plugin-alias": "^0.2.1",
        "esbuild-plugin-replace": "^1.4.0",
        "eslint": "8.56.0",
        "eslint-plugin-babel": "5.3.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsdoc": "48.0.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "front-matter-plugin-api-provider": "0.1.4-alpha",
        "keyword-extractor": "^0.0.28",
        "moment": "^2.30.1",
        "obsidian": "latest",
        "pdfjs-dist": "3.11.174",
        "prettier": "^3.3.3",
        "tslib": "2.6.2",
        "typescript": "5.3.3",
        "wink-eng-lite-web-model": "^1.8.0",
        "wink-nlp": "^2.3.0"
    },
    "dependencies": {
        "keyword-extractor": "^0.0.28",
        "natural": "^8.0.1",
        "wink-eng-lite-web-model": "^1.8.0",
        "wink-nlp": "^2.3.0"
    }
}
